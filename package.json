{
  "name": "@octocloud/typespec",
  "version": "0.1.18",
  "type": "module",
  "main": "dist/src/index.js",
  "tspMain": "lib/main.tsp",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "dependencies": {
    "@typespec/compiler": "1.0.0",
    "@typespec/http": "1.0.1",
    "@typespec/openapi": "1.0.0",
    "@typespec/openapi3": "1.0.0"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "@typespec/library-linter": "0.70.0",
    "eslint": "9.27.0",
    "prettier": "3.5.3",
    "typescript": "latest"
  },
  "scripts": {
    "build": "tsc -p . && npm run build:tsp",
    "watch": "tsc -p . --watch",
    "build:tsp": "tsp compile . --warn-as-error --import @typespec/library-linter",
    "test": "node --test ./dist/test/",
    "lint": "eslint src/ test/ --report-unused-disable-directives --max-warnings=0",
    "lint:fix": "eslint . --report-unused-disable-directives --fix",
    "format": "prettier .  --write",
    "format:check": "prettier --check .",
    "compile": "tsp compile lib/main.tsp",
    "compile:watch": "tsp compile . --watch",
    "generate-typespec": "tsp-openapi3 ./openapi.yml --output-dir ./tsp-output"
  },
  "publishConfig": {
    "access": "public"
  }
}
